@model IEnumerable<QA.Models.Post>
<div class="col-md-6 tn-right">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-6">
                <div class="tn-img">
                    <img src="~/Content/Images/@item.Images" />
                    <div class="tn-title">
                        <a href="@Url.Action("DetailPost","Home", new {id =item.IDPost })">@item.Topic</a>
                    </div>
                </div>
            </div>
        }
    </div>
</div>
